<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
   <bean id="consumer1" class="Projects.ConfigandBean.Consumer"> 
     <property name="p1" ref="product1"/>
   </bean>
   
     <bean id="product1" class="Projects.ConfigandBean.Product">
       <property name="productName" value="Apples"/>
        <property name="productId" value="123635"/>
        <property name="price" value="23"/>  
      </bean>
   
     
</beans>